@include "assembler/examples/utils.zasm"
@include "assembler/examples/stdio.zasm"

.entry_point main

main:
    andi $r4, 0
.loop:
    jal  getchar
    li   $r2, '\n'
    bnel $acc, $r2, .put_char

    li   $r5, 60
    div  $r4, $r5      ; ($r4 - $r4 % 60) + 60
    sub  $r4, $lo
    add  $r4, $r5

.put_char:
    stb  $acc, $r4(0)
    addi $r4, 1

    jl   .loop
    hlt
